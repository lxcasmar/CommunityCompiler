<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CommunityCompiler.Views.FavoritesView"
             Title="FavoritesView">
    <StackLayout Spacing="50">
        <Label 
            Text="This is the favorites page!!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Button
            x:Name="search"
            Text="Navigate to SearchView"
            Clicked="searchBtn_clicked" />

        <Button
            x:Name="setting"
            Text="Navigate to SettingView"
            Clicked="settingBtn_clicked" />

        <Button
            x:Name="home"
            Text="Navigate to HomeView"
            Clicked="homeBtn_clicked" />

    </StackLayout>
</ContentPage>
